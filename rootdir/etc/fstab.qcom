# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#<src>						<mnt_point>		<type>	<mnt_flags and options>						<fs_mgr_flags>
/dev/block/by-name/boot				/boot			emmc	defaults							defaults
/dev/block/by-name/recovery			/recovery		emmc	defaults							defaults

system						/system			ext4	ro								wait,logical,first_stage_mount
system_ext					/system_ext		ext4	ro								wait,logical,first_stage_mount
product						/product		ext4	ro								wait,logical,first_stage_mount
vendor						/vendor			ext4	ro								wait,logical,first_stage_mount
odm						/odm			ext4	ro								wait,logical,first_stage_mount
/dev/block/by-name/cache			/cache			f2fs	nosuid,nodev,noatime,inline_xattr,flush_merge,data_flush	wait,formattable,check
/dev/block/by-name/cache			/cache			ext4	nosuid,nodev,noatime						wait,formattable,check
/dev/block/by-name/misc				/misc			emmc	defaults							defaults
/dev/block/by-name/oem				/metadata		ext4	nosuid,nodev,noatime,discard,data=journal,commit=1		wait,check,formattable,first_stage_mount,check,metadata_csum
/dev/block/by-name/userdata			/data			f2fs	nosuid,nodev,noatime,discard,fsync_mode=nobarrier		latemount,wait,check,fileencryption=aes-256-xts,quota,formattable
/dev/block/by-name/userdata			/data			ext4	nosuid,nodev,noatime,discard,noauto_da_alloc			latemount,wait,check,fileencryption=aes-256-xts,quota,formattable

/dev/block/platform/soc/7824900.sdhci/by-name/persist		/mnt/vendor/persist		ext4	nosuid,nodev,noatime,no_sehash_xattr				wait,check
/dev/block/platform/soc/7824900.sdhci/by-name/dsp		/vendor/dsp			ext4	ro,nosuid,nodev							wait
/dev/block/platform/soc/7824900.sdhci/by-name/modem		/vendor/firmware_mnt		vfat	ro,context=u:object_r:firmware_file:s0,shortname=lower,uid=1000,gid=1000,dmask=227,fmask=337 wait
/dev/block/platform/soc/7824900.sdhci/by-name/config		/frp				emmc	defaults							defaults
